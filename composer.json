{
    "name": "alijumaan/shoppingcart",
    "description": "Laravel Shopping Cart",
    "authors": [
      {
        "name": "Ali AlQahtani",
        "email": "ali@alialqahtani.sa"
      }
    ],
    "type": "library",
    "require": {
      "illuminate/support": "^8|^9",
      "illuminate/session": "^8|^9",
      "illuminate/events": "^8|^9"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "orchestra/testbench": "^7",
      "mockery/mockery": "^1.5"
    },
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Alijumaan\\Shoppingcart\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Alijumaan\\Tests\\Shoppingcart\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
      "laravel": {
        "providers": [
          "Alijumaan\\Shoppingcart\\ShoppingcartServiceProvider"
        ],
        "aliases": {
          "Cart": "Alijumaan\\Shoppingcart\\Facades\\Cart"
        }
      }
    }
}
